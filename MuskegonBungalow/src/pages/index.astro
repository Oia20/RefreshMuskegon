---

<Layout>
  <main class="min-h-screen">
    <Navbar client:load />
    <Hero />
    <div class=" mb-0">
      <div id="about">
        <AboutProperty />
      </div>
    </div>

    <div id="gallery">
      <PhotoGallery client:load photos={photos} />
    </div>
    
    <div class="mx-auto space-y-24 overflow-x-hidden">
      <div id="amenities" class="-mx-4 sm:-mx-6 lg:-mx-8">
        <Amenities client:load/>
      </div>
      <div id="reviews">
        <Reviews />
      </div>
      <div id="calendar">
        <Calendar client:load />
      </div>
    </div>
  </main>
</Layout>
